{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/scripts/(.*)",
            "dest": "/public/scripts/$1"
        },
        {
            "src": "/styles/(.*)",
            "dest": "/public/styles/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/index.js"
        }
    ]
}